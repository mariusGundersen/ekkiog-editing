{
  "name": "ekkiog-editing",
  "version": "7.0.1",
  "description": "Editor for ekkiog",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "clean": "rimraf ./es ./js",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d js es",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/ekkiog-editing.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/ekkiog-editing/issues"
  },
  "homepage": "https://github.com/mariusGundersen/ekkiog-editing#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/node": "^7.0.66",
    "ava": "^2.2.0",
    "esm": "^3.2.25",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "buddy-tree": "^2.4.1",
    "ennea-tree": "^2.2.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.5.5"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "./ts/**/*.test.ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ]
  }
}
