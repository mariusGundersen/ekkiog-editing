{
  "name": "ekkiog-editing",
  "version": "7.0.0",
  "description": "Editor for ekkiog",
  "main": "js/main.js",
  "types": "es/main.d.ts",
  "module": "es/main.js",
  "scripts": {
    "clean": "rm -rf ./es ./js",
    "test": "ava",
    "tsc": "tsc",
    "compile": "npm run tsc && npm run babel",
    "babel": "babel --source-maps -d js es",
    "watch": "tsc -w & ava -w",
    "prepublish": "npm run clean && npm run compile && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mariusGundersen/ekkiog-editing.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariusGundersen/ekkiog-editing/issues"
  },
  "homepage": "https://github.com/mariusGundersen/ekkiog-editing#readme",
  "devDependencies": {
    "@types/node": "^7.0.66",
    "ava": "^0.25.0",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.24.1",
    "typescript": "^2.9.2"
  },
  "ava": {
    "files": [
      "es/**/*.test.js"
    ],
    "source": [
      "es/**/*.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "peerDependencies": {
    "babel-runtime": "^6.22.0"
  },
  "dependencies": {
    "buddy-tree": "^2.4.0",
    "ennea-tree": "^2.2.0"
  }
}
